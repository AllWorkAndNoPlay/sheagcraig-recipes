<script type="text/javascript">
	var parser = new UAParser();
	var result = parser.getResult();
	if (result.os.name=="Mac OS X") {
		// Grabs the middle digit of the version number and converts to an int ("10.8.2" => 8)
		var osMajorVersion = parseInt(result.os.version.split('.')[1]); 
		
		if (osMajorVersion >= 7) {
			$('#f4m2, #versionPick').hide();
			$('#downloadOlderF4M').show();
			$('#f4m3').css({
				float: 'none',
				margin: '0 auto'
			});
		} else {
			$('#f4m3, #versionPick').hide();
			$('#downloadNewerF4M').show();
			$('#f4m2').css({
				float: 'none',
				margin: '0 auto'
			});
		}
	} else if (result.os.name=="Windows") {
		$('#versionPick').html('<b><i>Flip4Mac is currently only available for Mac OS X users!</i></b>');
	}
</script>


